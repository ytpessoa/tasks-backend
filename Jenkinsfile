pipeline{
    agent any
    stages{
        stage('Inicio'){
            steps{
                bat 'echo Inicioooooooooo'
            }
        }
        stage('Meio'){
            steps{
                bat 'echo Meioooooo'
            }
        }
        stage('Fim'){
            steps{
                sleep(5)
                bat 'echo Fimmmmm'
            }
        }
    }
}